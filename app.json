{
  "name": "FitAppMobile",
  "displayName": "FitAppMobile",

  "android": {
    "package": "fit.app.mobile",
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#FFFFFF"
    },
    "enableShrinkResourcesInReleaseBuilds": true
  },

  "ios": {
    "bundleIdentifier": "fit.app.mobile",
    "supportsTablet": true
  },

  "assets": {
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "favicon": "./assets/favicon.png"
  },

  "buildProperties": {
    "android": {
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 23
    }
  },

  "eas": {
    "projectId": "ce12925e-db1c-4d64-b590-5248ae7f6644",
    "owner": "davide-bocc"
  }
}
