{
  "name": "FitApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prebuild": "expo prebuild",
    "build-apk-debug": "expo prebuild && cd android && .\\gradlew.bat assembleDebug && cd ..",
    "build-apk-release": "expo prebuild && cd android && .\\gradlew.bat assembleRelease && cd ..",
    "clean": "rimraf android/build node_modules package-lock.json",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "@expo/metro-config": "~0.10.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-firebase/firestore": "^22.4.0",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.26",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "expo": "~50.0.0",
    "expo-application": "~5.3.0",
    "expo-build-properties": "~0.8.3",
    "expo-modules-autolinking": "~1.5.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    //"expo-updates": "^0.18.19",
    "express": "^5.1.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^3.22.1",
    "react-native-sqlite-storage": "^6.0.1"
  },
  "devDependencies": {
