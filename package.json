{
  "name": "FitApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prebuild": "expo prebuild",
    "build-apk-debug": "expo prebuild && cd android && .\\gradlew.bat assembleDebug && cd ..",
    "build-apk-release": "expo prebuild && cd android && .\\gradlew.bat assembleRelease && cd ..",
    "clean": "rimraf android/build node_modules package-lock.json",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "@expo/metro-config": "~0.18.11",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "22.4.0",
    "@react-native-firebase/auth": "22.4.0",
    "@react-native-firebase/firestore": "^22.4.0",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.26",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "expo": "^51.0.0",
    "expo-application": "~5.9.1",
    "expo-build-properties": "~0.12.5",
    "expo-modules-autolinking": "~1.11.0",
    "expo-modules-core": "~1.12.26",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "express": "^5.1.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-sqlite-storage": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/metro-config": "^0.80.1",
    "react-native-gradle-plugin": "^0.71.19",
    "rimraf": "^5.0.0"
  }
